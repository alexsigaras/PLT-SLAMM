fun fib(number) do
   first = 0;
   second = 1;
   if number == 0 do
        print("0");
    elif number == 1 do
        print("1");
    else
        count = 1;
        while(count <= number) do
            temp = first+second;
            first = second;
            second = temp;
            count++;
        end
        print(first);
    end
end
 