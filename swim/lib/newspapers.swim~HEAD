# Online Newspapers Library

class newspapers do

	newspapers = ["http://www.gizmodo.com","http://www.engadget.com","http://www.arstechnica.com"];
	tags = [".post"];

	fun getPosts () do
		for each newspaper in newspapers.val do
			for each tag in tags.val do
				#print(@(tag, newspaper))
				print (@(tag, newspaper));
			end
		end
	end

end